<Window x:Class="Revanced_Builder.AppDownloadWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Revanced_Builder"
        mc:Ignorable="d"
        Title="Downloading Window" Height="140" Width="400">
    <Grid>
        <ProgressBar x:Name="DownloadProgress" HorizontalAlignment="Center" Height="23" VerticalAlignment="Top" Width="360" Margin="0,62,0,0"/>
        <Label x:Name="DownloadingLabel" Content="" HorizontalAlignment="Center" Margin="0,23,0,0" VerticalAlignment="Top"/>
        <Label x:Name="ProgressPercentageLabel" Content="" HorizontalAlignment="Center" Margin="0,61,0,0" VerticalAlignment="Top"/>
    </Grid>
</Window>
